{
  "name": "humile",
  "version": "0.2.14",
  "description": "node.js test framework on top of Jasmine",
  "bin": "src/index.js",
  "main": "src/index.js",
  "scripts": {
    "test": "src/index.js 'src/**/*.spec.js'",
    "test:full": "npm test -- --all && npm run lint",
    "lint": "eslint '**/*.js' --ignore-pattern 'node_modules'",
    "postversion": "npm run test:full && git push && git push --tags",
    "prepack": "npm run test:full"
  },
  "typings": "src/index.d.ts",
  "repository": "megahertz/humile",
  "files": [
    "src/*",
    "!__specs__"
  ],
  "keywords": [
    "test",
    "jasmine",
    "tdd",
    "bdd"
  ],
  "author": "Alexey Prokhorov",
  "license": "MIT",
  "bugs": "https://github.com/megahertz/humile/issues",
  "homepage": "https://github.com/megahertz/humile#readme",
  "engines": {
    "node": ">=10"
  },
  "devDependencies": {
    "eslint": "^6.6.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-plugin-import": "^2.18.2",
    "ts-node": "^5.0.1",
    "typescript": "^3.6.4"
  },
  "dependencies": {
    "concordance": "^4.0.0",
    "glob": "^7.1.4",
    "jasmine-core": "^3.5.0",
    "package-options": "^0.0.7"
  }
}
