{
  "name": "humile",
  "version": "0.3.0",
  "description": "node.js test framework on top of Jasmine",
  "bin": "src/index.js",
  "main": "src/index.js",
  "scripts": {
    "test": "node src/index.js",
    "test:full": "npm test -- --all && npm run lint && tsc --noEmit",
    "lint": "eslint \"src/**/*.js\"",
    "postversion": "git push && git push --tags",
    "preversion": "npm run test:full",
    "prepack": "npm run test:full"
  },
  "typings": "src/index.d.ts",
  "repository": "megahertz/humile",
  "files": [
    "src/*",
    "!__specs__"
  ],
  "keywords": [
    "test",
    "jasmine",
    "tdd",
    "bdd"
  ],
  "author": "Alexey Prokhorov",
  "license": "MIT",
  "bugs": "https://github.com/megahertz/humile/issues",
  "homepage": "https://github.com/megahertz/humile#readme",
  "engines": {
    "node": ">=10"
  },
  "devDependencies": {
    "@types/node": "^14.14.35",
    "eslint": "^7.22.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-plugin-import": "^2.22.1",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.3"
  },
  "dependencies": {
    "@types/jasmine": "^3.6.7",
    "concordance": "^4.0.0",
    "glob": "^7.1.6",
    "jasmine-core": "^3.6.0",
    "package-options": "^0.1.4"
  }
}
