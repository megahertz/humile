{
  "name": "humile",
  "version": "0.0.3",
  "description": "node.js test framework on top of Jasmine",
  "bin": "src/index.js",
  "main": "src/index.js",
  "scripts": {
    "test": "src/index.js 'src/**/*.spec.js'",
    "lint": "eslint '**/*.js' --ignore-pattern 'node_modules'"
  },
  "typings": "humile.d.ts",
  "repository": "megahertz/humile",
  "masks": [
    "src/*"
  ],
  "keywords": [
    "test",
    "jasmine",
    "tdd",
    "bdd"
  ],
  "author": "Alexey Prokhorov",
  "license": "MIT",
  "bugs": "https://github.com/megahertz/humile/issues",
  "homepage": "https://github.com/megahertz/humile#readme",
  "engines": {
    "node": ">=0.10"
  },
  "devDependencies": {
    "eslint": "^5.15.3",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.16.0"
  },
  "dependencies": {
    "glob": "^7.1.3",
    "jasmine-core": "^3.3.0"
  }
}
