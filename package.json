{
  "name": "humile",
  "version": "0.2.1",
  "description": "node.js test framework on top of Jasmine",
  "bin": "src/index.js",
  "main": "src/index.js",
  "scripts": {
    "test": "src/index.js 'src/**/*.spec.js'",
    "lint": "eslint '**/*.js' --ignore-pattern 'node_modules'",
    "postversion": "git push && git push --tags",
    "prepack": "npm run test"
  },
  "typings": "src/index.d.ts",
  "repository": "megahertz/humile",
  "files": [
    "src/*",
    "!src/**/*.spec.js"
  ],
  "keywords": [
    "test",
    "jasmine",
    "tdd",
    "bdd"
  ],
  "author": "Alexey Prokhorov",
  "license": "MIT",
  "bugs": "https://github.com/megahertz/humile/issues",
  "homepage": "https://github.com/megahertz/humile#readme",
  "engines": {
    "node": ">=0.10"
  },
  "devDependencies": {
    "eslint": "^5.16.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.17.2",
    "ts-node": "^5.0.1",
    "typescript": "^3.4.1"
  },
  "dependencies": {
    "concordance": "^4.0.0",
    "glob": "^7.1.3",
    "jasmine-core": "^3.3.0"
  }
}
